{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src/**/*", "astro.config.mjs"],
  "exclude": [
    "node_modules",
    "dist",
    "pages",
    "components",
    "lib",
    "interfaces",
    "utils",
    "hooks",
    "graphql",
    "test",
    "styles"
  ]
}
